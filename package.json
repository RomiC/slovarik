    "express": "^4.15.2",
    "express-graphql": "^0.6.4",
    "graphql": "^0.9.3",
    "graphql-relay": "^0.5.1",
    "pg": "^6.1.5",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-uikit-button": "^2.0.2",
    "react-uikit-form": "^2.0.3",
    "react-uikit-grid": "^2.0.3",
    "react-uikit-icons": "^2.0.3",
    "react-uikit-panel": "^2.0.2",
