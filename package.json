{
  "name": "slovarik",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "beforestart": "npm run client:build",
    "start": "index.js",
    "dev:client": "npm-run-all -p watch:*",
    "dev:server": "nodemon --inspect index.js",
    "build": "cross-env NODE_ENV=production npm run build:js",
    "build:js": "rollup -c ./client/rollup.config.js",
    "watch:js": "npm run build:js -- -w",
    "watch:client": "browser-sync start --server --serveStatic ./client/ --files ./client/dist/ --index ./client/index.htm"
  },
  "repository": "git@github.com:RomiC/slovarik.git",
  "author": "Roman Charugin <roman_charugin@epam.com>",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^5.0.0",
    "dataloader": "^1.3.0",
    "element-react": "^1.0.1",
    "element-theme-default": "^1.3.4",
    "express": "^4.15.2",
    "express-graphql": "^0.6.4",
    "graphql": "^0.9.3",
    "graphql-relay": "^0.5.1",
    "pg": "^6.1.5",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1",
    "sequelize": "^3.30.4"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.18.12",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "postcss-import": "^10.0.0",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.5.3",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^3.2.2"
  }
}
